{
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py",
      "methods": ["POST", "GET", "OPTIONS"]
    },
    {
      "src": "/api",
      "dest": "/api/index.py",
      "methods": ["POST", "GET", "OPTIONS"]
    }
  ]
}
